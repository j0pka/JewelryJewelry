<Window x:Class="BocharovaJewelryBocharovaJewelry.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BocharovaJewelryBocharovaJewelry"
        mc:Ignorable="d"
        Title="MainWindow" MinHeight="500" MinWidth="900" MaxWidth="901" FontFamily="Comic Sans MS" Icon="Resources/logo.png">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Image Source="/Resources/logo.png" HorizontalAlignment="Left"></Image>
        <TextBlock Text="ООО Украшения" FontSize="30" HorizontalAlignment="Center"
 VerticalAlignment="Center" />
        <Button  Content="Назад" Name="BtnBack" HorizontalAlignment="Right" Click="BtnBack_Click"/>

        <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden" ></Frame>
    </Grid>
</Window>
